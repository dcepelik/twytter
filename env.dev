#!/hint/sh

# Port where the API is listening.
export TWYTTER_PORT=9090

# Database user. No, normally you don't run as the postgres user,
# because it's the PostgreSQL equivalent of root.
export TWYTTER_DB_USER=postgres

# Database password. No, this is not a secure password.
export TWYTTER_DB_PASSWORD=1234

# Database host.
export TWYTTER_DB_HOST=127.0.0.1

# Database port.
export TWYTTER_DB_PORT=5432
